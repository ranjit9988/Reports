    <DataSet Name="GetContractorsByDistrict">
      <Query>
        <DataSourceName>VariableDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Dist">
            <Value>=Parameters!Dist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          -- get all Contractors for a District

          -- Declare @Dist int; Set @Dist = 56 ;


          Select Distinct CT.ContractorID, CT.Name
          From Contracts CO
          join Contractors CT on CO.ContractorID = CT.ContractorID
          Where (CO.IsDeleted= 0 OR CO.IsDeleted IS NULL) And CO.DistrictID = @Dist
          Order By CT.Name
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ContractorID">
          <DataField>ContractorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetAllContracts">
      <Query>
        <DataSourceName>VariableDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Contractor">
            <Value>=Parameters!Contractor.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dist">
            <Value>=Parameters!Dist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          -- get all Contracts for a given Contractor / District

          --Declare @Dist int; Set @Dist = 56;
          --Declare @Contractor int; Set @Contractor = 1070

          Select CO.ContractID, CT.Name as CtName, C.College, P.ProjectID, P.ProjectNumber, P.ProjectName,
          CO.Description as CoDesc, CO.ContractDate, CO.Status as CoStatus,
          (Select IsNull(Sum(Amount),0) From ContractLineItems CLI Where CLI.IsDeleted= 0 And CO.IsDeleted= 0 And CLI.ContractID = CO.ContractID and CLI.LineType = 'Contract' and Reimbursable = 0) as ContAmount,
          (Select IsNull(Sum(Amount),0) From ContractLineItems CLI Where CLI.IsDeleted= 0 And CO.IsDeleted= 0 And CLI.ContractID = CO.ContractID and CLI.LineType = 'Contract' and Reimbursable = 1) as ReimbAmount,
          (Select Sum(Amount) From ContractDetail Where ContractDetail.IsDeleted= 0 And CO.IsDeleted= 0 And ContractID = CO.ContractID) as AmendTot
          From Contracts CO
          left outer join Projects P on CO.ProjectID = P.ProjectID
          left outer join Colleges C on CO.CollegeID = C.CollegeID
          left outer join Contractors CT on CO.ContractorID = CT.ContractorID
          Where (P.IsDeleted= 0 OR P.IsDeleted IS NULL) And (C.IsDeleted= 0 OR C.IsDeleted IS NULL) And (CO.IsDeleted= 0 OR CO.IsDeleted IS NULL) And CO.ContractorID in (@Contractor) and CO.DistrictID = @Dist
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ContractID">
          <DataField>ContractID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CtName">
          <DataField>CtName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="College">
          <DataField>College</DataField>
